package com.example.demo;

public class Student {
	
	int id;
	
	Mathcheat mathCheat;
	
	

	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}

	public void setMathCheat(Mathcheat mathCheat) {
		this.mathCheat = mathCheat;
	}

	public void cheating() {
		mathCheat.mathCheat();
	}

}
